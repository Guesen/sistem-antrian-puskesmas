{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Sistem Antrian Puskesmas",
  "version": "1.0.0",
  "identifier": "com.puskesmas.antrian",
  "build": {
    "frontendDist": "../build/client",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev:frontend",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Sistem Antrian Puskesmas Mrebet",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "Â© 2024 Puskesmas Mrebet",
    "category": "Medical",
    "shortDescription": "Sistem Antrian Digital Puskesmas",
    "longDescription": "Aplikasi sistem antrian digital untuk Puskesmas Mrebet Kabupaten Purbalingga"
  }
}
